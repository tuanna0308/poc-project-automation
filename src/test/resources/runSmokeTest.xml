<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Run Smoke Test for PoC Project" parallel="false">
    <listeners>
        <listener class-name="com.epam.poc.utilities.listeners.TestListener"/>
    </listeners>
    <parameter name="pageUrl" value="http://localhost:8080/"/>
    <test name = "Test template">
        <classes>
            <class name = "com.epam.poc.tests.TestTemplate" />
        </classes>
    </test>
</suite>